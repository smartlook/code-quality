{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:base"],
	"assignees": ["samialdury"],
	"labels": ["dependencies", "tooling"],
	"onboarding": false,
	"repositories": ["smartlook/code-quality"],
	"packageRules": [
		{
			"matchPackagePatterns": ["eslint", "prettier"],
			"labels": ["styling"]
		},
		{
			"matchPackagePatterns": ["eslint"],
			"groupName": "eslint"
		}
	]
}
